{
  "name": "@krieselreihe/website",
  "version": "1.0.0",
  "description": "The super awesome website for Krieselreihe Games.",
  "private": true,
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy && prettier --write build",
    "lint": "eslint '**/*.html' && stylelint 'src/**/*.css'",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "ci": "pnpm run lint && pnpm run format:check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krieselreihe/website.git"
  },
  "keywords": [
    "website",
    "games",
    "krieselreihe",
    "selenium"
  ],
  "author": "Krieselreihe Games <info@krieselreihe.com> (https://krieselreihe.com)",
  "contributors": [
    "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krieselreihe/website/issues"
  },
  "homepage": "https://krieselreihe.com",
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-html": "7.1.0",
    "luxon": "3.4.4",
    "prettier": "3.1.0",
    "stylelint": "15.11.0",
    "stylelint-prettier": "4.1.0"
  }
}
