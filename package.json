{
  "name": "@krieselreihe/website",
  "version": "1.0.0",
  "description": "The super awesome website for Krieselreihe Games text adventures.",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build && next export",
    "dev": "next",
    "test": "jest --passWithNoTests",
    "commit": "git-cz",
    "lint": "eslint '**/*.js' && stylelint '**/*.jsx'",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "ci": "pnpm run lint && pnpm run format:check && pnpm run test",
    "preinstall": "node ./scripts/ensure-package-manager.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krieselreihe/website.git"
  },
  "keywords": [
    "website",
    "games",
    "krieselreihe",
    "text",
    "adventure",
    "selenium"
  ],
  "author": "Krieselreihe Games <info@krieselreihe.com> (https://krieselreihe.com)",
  "contributors": [
    "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krieselreihe/website/issues"
  },
  "homepage": "https://krieselreihe.com",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "next": "12.1.6",
    "number-generator": "4.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "babel-jest": "28.1.1",
    "babel-plugin-styled-components": "2.0.7",
    "commitizen": "4.2.4",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.1",
    "jest": "28.1.1",
    "jest-progress-bar-reporter": "1.0.24",
    "postcss": "8.4.14",
    "postcss-syntax": "0.36.2",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "stylelint": "14.9.1",
    "stylelint-config-recommended": "8.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
