{
  "name": "@krieselreihe/website",
  "version": "1.0.0",
  "description": "The super awesome website for Krieselreihe Games text adventures.",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build && next export",
    "postbuild": "cp deployment/* out/",
    "dev": "next",
    "test": "jest --passWithNoTests",
    "commit": "git-cz",
    "lint": "eslint '**/*.js' && stylelint '**/*.jsx'",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,jsx,css,md,json,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krieselreihe/website.git"
  },
  "keywords": [
    "website",
    "games",
    "krieselreihe",
    "text",
    "adventure",
    "selenium"
  ],
  "author": "Krieselreihe Games <info@krieselreihe.com> (https://krieselreihe.com)",
  "contributors": [
    "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krieselreihe/website/issues"
  },
  "homepage": "https://krieselreihe.com",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "next": "9.2.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "styled-components": "5.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "babel-jest": "25.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "commitizen": "4.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.2",
    "jest": "25.1.0",
    "jest-dot-reporter": "1.0.14",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "stylelint": "13.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.9.0"
  }
}
