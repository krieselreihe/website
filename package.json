{
  "name": "@krieselreihe/website",
  "version": "1.0.0",
  "description": "The super awesome website for Krieselreihe Games text adventures.",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build && next export",
    "dev": "next",
    "test": "jest --passWithNoTests",
    "commit": "git-cz",
    "lint": "eslint '**/*.js' && stylelint '**/*.jsx'",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "ci": "pnpm run lint && pnpm run format:check && pnpm run test",
    "preinstall": "node ./scripts/ensure-package-manager.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krieselreihe/website.git"
  },
  "keywords": [
    "website",
    "games",
    "krieselreihe",
    "text",
    "adventure",
    "selenium"
  ],
  "author": "Krieselreihe Games <info@krieselreihe.com> (https://krieselreihe.com)",
  "contributors": [
    "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krieselreihe/website/issues"
  },
  "homepage": "https://krieselreihe.com",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "next": "10.2.0",
    "number-generator": "4.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "styled-components": "5.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "babel-jest": "26.6.3",
    "babel-plugin-styled-components": "1.12.0",
    "commitizen": "4.2.3",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "jest": "26.6.3",
    "jest-progress-bar-reporter": "1.0.20",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
