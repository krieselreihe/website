{
  "name": "@krieselreihe/website",
  "version": "1.0.0",
  "description": "The super awesome website for Krieselreihe Games text adventures.",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build && next export",
    "dev": "next",
    "test": "jest --passWithNoTests",
    "commit": "git-cz",
    "lint": "eslint '**/*.js' && stylelint '**/*.jsx'",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,jsx,css,md,json,yml}\"",
    "ci": "yarn lint && yarn format:check && yarn test",
    "preinstall": "node ./scripts/ensure-package-manager.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krieselreihe/website.git"
  },
  "keywords": [
    "website",
    "games",
    "krieselreihe",
    "text",
    "adventure",
    "selenium"
  ],
  "author": "Krieselreihe Games <info@krieselreihe.com> (https://krieselreihe.com)",
  "contributors": [
    "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krieselreihe/website/issues"
  },
  "homepage": "https://krieselreihe.com",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "babel-jest": "26.3.0",
    "babel-plugin-styled-components": "1.11.1",
    "commitizen": "4.1.5",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.5",
    "jest": "26.3.0",
    "jest-progress-bar-reporter": "1.0.18",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
